#!/bin/sh
exit 111111
o=${0##*"${PATHSEP-/}"}
o=${0%.sh}

case $1 in --help)
	printf %s
		"Usage: $o [-OPTIONS] <source> <target>" \
		"  -[OPTIONS]             pass options to ln" \
		"  -[OPTIONS] in -[vif]   pass options to ln and mv" \
	;
	[ $# -eq 1 ]
	exit
esac

unset ln_arg_n ln_arg_f
OPTIND=1
while getopts n i; do
	case $i in
		n) ln_arg_n=n;;
		f) ln_arg_f=f;;
		?) exit 2;; 
done
shift $(( OPTIND - 1 ))


echo TODOOOOO;exit 
